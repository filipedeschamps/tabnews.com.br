{
  "dependencies": {
    "next": "11.0.1",
    "node-pg-migrate": "5.10.0",
    "pg": "8.6.0",
    "react": "17.0.2",
    "react-confetti": "6.0.1",
    "react-dom": "17.0.2",
    "react-icons": "4.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.14.7",
    "autoprefixer": "10.3.1",
    "axios": "0.21.1",
    "concurrently": "6.2.0",
    "dotenv": "10.0.0",
    "jest": "27.0.6",
    "kill-port": "1.6.1",
    "postcss": "8.3.5",
    "tailwindcss": "2.2.4",
    "tree-kill": "1.2.2"
  },
  "scripts": {
    "dev": "concurrently -n docker,next -k \"npm:services:up\" \"npm run next\"",
    "next": "next dev",
    "services:up": "docker-compose -f docker-compose.development.yml up",
    "build": "next build",
    "start": "next start -p 8080",
    "test": "jest --runInBand --forceExit",
    "test:watch": "jest --runInBand --watch"
  },
  "name": "tabnews.com.br",
  "description": "Conteúdos para quem vive de programação e tecnologia.",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/filipedeschamps/tabnews.com.br.git"
  },
  "keywords": [
    "news"
  ],
  "author": "Filipe Deschamps",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/filipedeschamps/tabnews.com.br/issues"
  },
  "homepage": "https://www.tabnews.com.br/",
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "."
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/.next"
    ],
    "testTimeout": 120000,
    "slowTestThreshold": 40000,
    "setupFiles": [
      "dotenv/config"
    ]
  }
}
